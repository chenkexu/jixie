<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/window_system_color"
        android:orientation="horizontal"
        android:padding="@dimen/topbar_padding">

        <ImageView
            android:id="@+id/base_top_bar_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:paddingBottom="3dp"
            android:paddingLeft="5dp"
            android:paddingRight="10dp"
            android:paddingTop="3dp"
            android:src="@mipmap/left_btn_white"
            android:visibility="gone" />

        <TextView
            android:id="@+id/base_top_bar_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/window_topbar_text_color"
            android:textSize="@dimen/topbar_textsize" />

        <TextView
            android:id="@+id/base_top_bar_title_ge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="4dp"
            android:layout_toRightOf="@id/base_top_bar_title"
            android:background="@drawable/chengyunren_geren_bg"
            android:padding="2dp"
            android:text="@string/ge"
            android:textColor="@color/window_topbar_text_color"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/base_top_bar_right2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="21dp"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/base_top_bar_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@id/base_top_bar_right2"
            android:visibility="gone" />

        <TextView
            android:id="@+id/base_top_bar_righttext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:textColor="@color/window_topbar_text_color"
            android:textSize="@dimen/topbar_textsize"
            android:visibility="gone" />

    </RelativeLayout>



        <LinearLayout
