<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/icon_selecter_register_bg"
    android:orientation="vertical">

    <include
        android:id="@+id/selecter_register_layout_topbar"
        layout="@layout/base_top_bar_center" />

    <View
        android:id="@+id/selecter_register_layout_fuzhu"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_centerVertical="true" />

    <TextView
        android:id="@+id/id_selecter_register_gerenkaidian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/selecter_register_layout_fuzhu"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="35dp"
        android:background="@mipmap/icon_selecter_register_geren"
        android:gravity="center"
        android:text="@string/gerenkaidian"
        android:textColor="@color/cffffff"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/id_selecter_register_qiyekaidian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/selecter_register_layout_fuzhu"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:background="@mipmap/icon_selecter_register_gongsi"
        android:gravity="center"
        android:text="@string/qiyekaidian"
        android:textColor="@color/cffffff"
        android:textSize="20sp" />
</RelativeLayout>