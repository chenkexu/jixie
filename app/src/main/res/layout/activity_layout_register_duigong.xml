<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/base_top_bar_center" />

    <ListView
        android:id="@+id/id_regisger_duigong_listview"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:overScrollMode="never"
        android:scrollbars="none" />

    <Button
        android:id="@+id/id_register_duigong_next"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/tiezi_details_fasong_bg"
        android:text="@string/querenduigongdakuan"
        android:textColor="@color/cffffff"
        android:textSize="16sp" />
    
</LinearLayout>